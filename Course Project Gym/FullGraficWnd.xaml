<Window x:Class="Course_Project_Gym.FullGraficWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Course_Project_Gym"
        mc:Ignorable="d"
        Title="FullGraficWnd" Height="450" Width="650" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Calendar x:Name="CalendarSmall" SelectedDatesChanged="Calendar_SelectedDatesChanged" Grid.Column="0" Margin="0,0,43,10"/>
        <ScrollViewer VerticalScrollBarVisibility="Auto" CanContentScroll="True" Margin="301,0,10,0" Grid.ColumnSpan="2">
            <StackPanel x:Name="CoachesPanel"/>
        </ScrollViewer>
    </Grid>
</Window>
